msgstr ""

#: ../../_pages/jmv_propTest2.rst:69
msgid "``ciBayes``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:69
msgid "``TRUE`` or ``FALSE`` (default), provide Bayesian credible intervals"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:72
msgid "``ciBayesWidth``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:72
msgid "a number between 50 and 99.9 (default: 95), the credible interval width"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:75
msgid "``postPlots``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:75
