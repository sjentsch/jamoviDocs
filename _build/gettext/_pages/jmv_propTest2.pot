msgid "``data``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:40
msgid "the data as a data frame"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:42
msgid "``vars``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:42
msgid "a vector of strings naming the variables of interest in ``data``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:45
msgid "``areCounts``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:45
msgid "``TRUE`` or ``FALSE`` (default), the variables are counts"
msgstr ""

