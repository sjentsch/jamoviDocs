msgid "Usage"
msgstr ""

#: ../../_pages/jmv_ttestIS.rst:46
msgid "Arguments"
msgstr ""

#: ../../_pages/jmv_ttestIS.rst:49
msgid "``data``"
msgstr ""

#: ../../_pages/jmv_ttestIS.rst:49
msgid "the data as a data frame"
msgstr ""

#: ../../_pages/jmv_ttestIS.rst:51
msgid "``vars``"
msgstr ""

#: ../../_pages/jmv_ttestIS.rst:51
